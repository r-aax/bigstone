<GUI>:

    FloatLayout:

        cols: 1
        size: root.width, root.height
        pos: 0, 0

        GridLayout:

            cols: 5
            size_hint_y: 0.05

            Button:
                text: "A"
                on_press: root.OnClickA()

            Button:
                text: "B"
                on_press: root.OnClickB()

            Button:
                text: "C"
                on_press: root.OnClickC()

            Button:
                text: "D"
                on_press: root.OnClickD()

            Button:
                text: "E"
                on_press: root.OnClickE()
